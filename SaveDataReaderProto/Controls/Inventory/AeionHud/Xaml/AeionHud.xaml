<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:aHud="clr-namespace:SaveDataReaderProto.Controls.Inventory.AeionHud">
    <Style TargetType="{x:Type aHud:AeionHud}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type aHud:AeionHud}">
                    <Border x:Name="PART_AEMax" Height="8" Background="#FF0F3B53">
                        <Rectangle x:Name="PART_AECurrent" Fill="#FFE4DD1D" />
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>